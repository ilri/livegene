{
    "name": "ilri/livegene",
    "description": "ILRI LiveGene",
    "type": "project",
    "homepage": "https://livegene.ilri.org",
    "license": "proprietary",
    "authors": [
        {
            "name": "Cezar Pendarovski",
            "email": "cezar.pendarovski@roslin.ed.ac.uk",
            "role": "Lead developer"
        },
        {
            "name": "Johanna Benndorf",
            "email": "jbennado@exseed.ed.ac.uk",
            "role": "Developer"
        },
        {
            "name": "David Mirieri Mogaka",
            "email": "d.mogaka@cgiar.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer-runtime-api": "^2",
        "api-platform/api-pack": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nesbot/carbon": "^2.22",
        "sensio/framework-extra-bundle": "^5.1",
        "simplethings/entity-audit-bundle": "^1.0",
        "sonata-project/admin-bundle": "^3.45",
        "sonata-project/doctrine-orm-admin-bundle": "^3.8",
        "sonata-project/media-bundle": "^3.18",
        "sonata-project/user-bundle": "^4.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "liip/functional-test-bundle": "^3.2",
        "liip/test-fixtures-bundle": "^1.3",
        "phpunit/phpunit": "^8.5",
        "sonata-project/easy-extends-bundle": "^2.5",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
