{
    "name": "livegene",
    "description": "ILRI LiveGene",
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.12.7",
        "@babel/runtime": "^7.8.7",
        "@babel/types": "^7.8.7",
        "@fancyapps/fancybox": "^3.5.6",
        "@symfony/webpack-encore": "^0.22.0",
        "@vue/test-utils": "^1.0.0-beta.31",
        "animate.css": "^3.7.2",
        "axios": "^0.21.2",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^8.2.6",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.0.6",
        "babel-preset-vue": "^2.0.2",
        "bootstrap": "^4.5.3",
        "bootstrap-vue": "^2.21.1",
        "chromedriver": "^96.0.0",
        "ckeditor": "^4.12.1",
        "core-js": "^3.1.2",
        "cross-env": "^7.0.0",
        "d3": "^5.16.0",
        "d3-array": "^2.4.0",
        "d3-cloud": "^1.2.5",
        "d3-sankey": "^0.12.3",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-vue": "^6.2.1",
        "flag-icon-css": "^3.3.0",
        "flush-promises": "^1.0.2",
        "font-awesome": "^4.7.0",
        "jest": "^24.9.0",
        "jest-transform-stub": "^2.0.0",
        "jquery": "^3.5.0",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.21",
        "nightwatch": "^1.7.8",
        "nprogress": "^0.2.0",
        "popmotion": "^8.7.1",
        "regenerator-runtime": "^0.13.2",
        "selenium-server": "^3.141.59",
        "topojson-client": "^3.1.0",
        "uuid": "^8.3.2",
        "versor": "^0.0.4",
        "vue": "^2.6.11",
        "vue-jest": "^3.0.7",
        "vue-loader": "15.0.11",
        "vue-router": "^3.0.6",
        "vue-server-renderer": "^2.6.11",
        "vue-template-compiler": "^2.6.11",
        "vuex": "^3.1.1",
        "webpack-notifier": "^1.12.0"
    },
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "dev-server": "encore dev-server",
        "dev": "encore dev",
        "watch": "encore dev --watch",
        "build": "encore production --progress",
        "test:unit": "jest --verbose assets/spa",
        "test:e2e": "nightwatch --config assets/spa/tests/e2e/nightwatch.conf.js --env chrome",
        "lint": "eslint assets/spa --ext .js,.vue,"
    },
    "jest": {
        "testMatch": [
            "**/assets/spa/tests/unit/**/*.spec.js"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest",
            "^.+\\.vue$": "vue-jest",
            "^.+\\.(css|png|jpg|svg|ttf|woff|woff2)$": "jest-transform-stub"
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "assets/spa/**/*.{js,vue}",
            "!assets/spa/main.js",
            "!assets/spa/tests/**/*"
        ],
        "coverageReporters": [
            "html",
            "text"
        ],
        "coverageDirectory": "assets/spa/tests/unit/coverage"
    },
    "browserslist": [
        "> 0.5%",
        "last 2 versions",
        "Firefox ESR",
        "not dead"
    ]
}
